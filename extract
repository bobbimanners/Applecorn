#!/bin/sh
for file in APPLECORN LOADER MAINMEM AUX.AMOS.INIT AUX.AMOS.KRNL AUX.HOSTFS AUX.IRQ.VECS AUX.MISC AUX.MOS.WS AUX.OSBW.JGH AUX.OSBW.ORIG AUX.VDU; do
  cadius extractfile applecorn.po /APPLECORN/${file}.S .
  rm _FileInformation.txt
  cadius clearhighbit ${file}.S\#040000
  cadius indentfile ${file}.S\#040000
done
